# BEGIN_KITTY_THEME
# Gruvbox Dark
include current-theme.conf
# END_KITTY_THEME

background_image /home/shrmrm/Wallps/spaw.png

background_image_layout centered
background_tint 0.1
background_tint_gaps 0.4
background_opacity 0.80
background_blur 30

font_size 13
font_family Monaspace Neon Regular
disable_ligatures never
font_features Monaspace Neon Regular +liga
line_height 1.1

map ctrl+shift+n new_window
map ctrl+shift+t new_tab
map ctrl+shift+s kitten hints
